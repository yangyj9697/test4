package ch03.sec11;

public class LogicAndBinary {
	public static void main(String[] args) {
		
		boolean a = true;
		boolean b = true;
		
		System.out.println("a&&b : " +(a&&b));
		System.out.println("a&&b : " +(a||b));
		System.out.println("a&&b : " +(a==b));
		System.out.println("a&&b : " +(a!=b));
	}
}
